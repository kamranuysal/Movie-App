{
  "compilerOptions": {
    "composite": true,
    "target": "es2016",
    "module": "ESNext",
    "moduleResolution": "node10",
    "allowImportingTsExtensions": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
